{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "My flawless Nuxt.js project",
  "author": "key46",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch server --exec ts-node server/index",
    "build": "nuxt build",
    "build:server": "tsc",
    "start": "cross-env NODE_ENV=production ts-node server/index",
    "generate": "npm run build:server && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "intersection-observer": "^0.5.1",
    "magic-snowflakes": "^4.1.0",
    "nuxt": "^2.0.0",
    "serverless-http": "^1.8.0",
    "v-lazy-image": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^10.12.15",
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "nodemon": "^1.11.0",
    "nuxt-imagemin": "^1.0.0",
    "prettier": "1.14.3",
    "tailwindcss": "~0.6.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
}
